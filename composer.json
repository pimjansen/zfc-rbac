{
    "name": "pimjansen/zfc-rbac",
    "description": "Zend Framework 2 Module that provides a layer of features of Laminas\\Permissions\\Rbac",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2",
        "rbac",
        "permissions"
    ],
    "homepage": "http://www.github.com/pimjansen/zfc-rbac",
    "require": {
        "php": "^7.4",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-stdlib": "^3.1",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.7",
        "phpunit/phpunit": "^8.5.2",
        "phpspec/prophecy": "^1.10",
        "friendsofphp/php-cs-fixer": "^2.9.3",
        "doctrine/common": "^2.4",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ZfcRbac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfcRbacTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev",
            "dev-develop": "3.0-dev"
        },
        "zf": {
            "component": "ZfcRbac",
            "config-provider": "ZfcRbac\\ConfigProvider"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test",
            "@header"
        ],
        "cs": "php-cs-fixer fix -v --diff --dry-run",
        "cs-fix": "php-cs-fixer fix -v --diff",
        "test": "phpunit",
        "header": "docheader check src test"
    }
}
